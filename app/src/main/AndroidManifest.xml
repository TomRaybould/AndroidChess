<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.miketomkowich.chess72android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gamestage"
            android:label="Home Screen"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".list_by_date"
            android:label="List Of Games By Date"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".list_by_name"
            android:label="List Of Games By Name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>